void setup() {
  display.setBrightness(bright); //Set the display brightness (0-7)
  display.clear(); // Clear the display:
  delay(100);
  Wire.begin(); //Arduino as I2C master
  delay(100);
   while (myAHT10.begin() != true) {
    display.setSegments(alphabet+4, 1, 1); //AHT 10 initialize error
    display.setSegments(alphabet+17, 1, 2);
    display.setSegments(alphabet+17, 1, 3);
    delay(200);
  }
  Wire.setClock(400000); //experimental I2C speed! 400KHz, default 100KHz
  
  timeStart = millis(); //start time tick
  cycleStart = millis();
  pinMode(BZ, OUTPUT);
  digitalWrite(BZ, LOW); //turn off buzzer
  pinMode(modeSW, INPUT_PULLUP);
  pinMode(incSW, INPUT_PULLUP);
  DDRD = B11111110; //DAC bit 1-7
  PORTD &= B00000001;
  PORTD =0;
}

void pcmPlay(uint16_t pStart, uint16_t pEnd) {
  digitalWrite(BZ, HIGH); //turn on buzzer
  s16Sam=0,index=0,stepSize=7;
  for (uint16_t k=pStart*2;k<pEnd*2;k++){
    
//get adSam nibble
    adSam=pgm_read_word_near(adpcm+int(k/2));
    if(k%2==0)adSam&=0x0F; //get adpcm from low nibble
    else adSam>>=4; //get adpcm from high nibble
    
//calculate current s16Sam from previous s16Sam+delta=(adSam+1/2)*stepSize/4
    delta=0;  
    if (adSam & 4)delta+=stepSize;
    if (adSam & 2)delta+=stepSize >>1;
    if (adSam & 1)delta+=stepSize >>2; 
    delta += stepSize >>3;
    if (adSam & 8)delta=-delta;
    s16Sam=constrain(s16Sam+delta,-32768,32767);

//get new stepsize
    index=constrain(index+indexTbl[adSam],0,88);
    stepSize=stepSizeTbl[index];
    
 //output unsigned 8 bit PCM 
    PORTD =0x80^highByte(s16Sam); //convert from signed 16bitPCM to unsigned 8bitPCM
    delayMicroseconds(114);//114us+decoder11us=125us for 8KHZ sample rate
  }
  PORTD=0;
  digitalWrite(BZ, LOW); //turn off buzzer
}
  
void loop() {
//quarter chime
  if ((toneCount>0)&&((millis()-toneStart) > toneDelay)) {
    toneStart=millis(),toneDelay=667;
    if (sound[lastTone-toneCount]<99) pcmPlay(ptr[sound[lastTone-toneCount]],ptr[sound[lastTone-toneCount]+1]);
    toneCount--;
    if (toneCount==0) toneDelay=12000;
  }
  
//hour counting chime  
  if ((toneCount==0)&&(tickCount>0)&&((millis()-toneStart) > toneDelay)) {
    toneStart=millis(), toneDelay=4000;
    pcmPlay(ptr[4],ptr[5]); //E3
    tickCount--;
  }

//one minute pass
  if(((millis() - timeStart)>=minTime)||((timeStart > millis()) && (millis() >=minTime))) { //one minute, also handle millis overflow
    timeStart = timeStart+minTime;
    cycle=0;
    tmrAlarm=0; //turn off timer alarm
    cAlarm=0; //turn off clock alarm
    if (setMode==0) {
      if (tmr == 1) tmrAlarm = 1;
      if (tmr > 0) tmr--;
    }
    m++;
    if (m == 60){
      m = 0, h++;
      tickCount=0;
      if((h>6)&&(h<23)&&(chimeST==3)) {
        tickCount=h%12;
        if(tickCount==0) tickCount=12;
      }
    }
    if ((h>6)&&(h<23)&&(((m%15==0)&&(chimeST>1))||((m==0)&&(chimeST>0)))) {
      lastTone=(int((m+45)/15)%4)*5+5;
      for (int i=0;i<lastTone;i++) sound[i]=note[notePtr[int((m+45)/15)%4]+i]; //chime
      toneCount=lastTone,toneDelay=667,toneStart=millis();
    }
    if (h == 24) {
      h = 0, dd++, dow++;
      if (dow==7) dow=0;
      switch(mmm) {
        case 4:
        case 6:
        case 9:
        case 11:
          if (dd==31) dd=1, mmm++;
          break;
        case 2:
          if ((yy%4)==0) {
            if (dd==30) dd=1, mmm++;
          }
          else if (dd==29) dd=1, mmm++;
          break;
        default:
          if (dd==32) dd=1, mmm++;
          break;
      }
      if (mmm==13) mmm=1, yy++;
    }
    if ((h == aH) && (m == aM)) cAlarm=1;
  }

//normal display cycle  
  if((millis() - cycleStart)>=cycleTime && setMode==0) {
    cycleStart = millis();
    if(tmrAlarm==1 && cycle<4) pcmPlay(ptr[cycle],ptr[cycle+1]); //B3-E4-F#4-G#4
    if(cAlarm==1 && cycle<4) cycle%2==0 ? pcmPlay(ptr[0],ptr[1]): pcmPlay(ptr[3],ptr[4]); //B3-G#4
    switch(cycle) {
      case 0:
      case 2:
        cycleTime=500;
        display.showNumberDecEx(h*100+m, 0b01000000, true, 4, 0);
        break;
      case 1:
      case 3:
        cycleTime=500;
        display.showNumberDecEx(h*100+m, 0b00000000, true, 4, 0);
        if ((tmr == 0)&&(cycle==3)) cycle++; //if no timer, skip comming show timer cycle
        break;
      case 4:
        cycleTime=2000;
        display.setSegments(alphabet+19, 1, 0); //show remain time
        display.showNumberDec(tmr, true, 3, 1);
        break;
      case 5:
        cycleTime=2000;
        temperature = myAHT10.readTemperature(AHT10_FORCE_READ_DATA)+tempOffset;
        display.showNumberDecEx(temperature*10, 0b01000000, false, 3, 0);
        display.setSegments(symbol+3, 1, 3);
        break;
      case 6:
        cycleTime=2000;
        humidity = myAHT10.readHumidity(AHT10_USE_READ_DATA)+humOffset;
        display.showNumberDecEx(humidity*10, 0b01000000, false, 3, 0);
        display.setSegments(symbol+4, 1, 3);
        break;
    }
    cycle++;
    if (cycle==7) cycle=0;
  }

//press both inc and mode switch
  modeST = digitalRead(modeSW);
  incST = digitalRead(incSW);
  if (modeST == LOW || incST == LOW) {
    if (tmrAlarm == 1 || cAlarm ==1) {
      tmrAlarm = 0; //if alarm on, press any switch to turn alarm off
      cAlarm = 0;
    }
    else if (modeST == LOW && incST == LOW) { 
      switch(setMode) {
        case 0:
          break;
        case 1:
          tmr=0;
          break;
        case 2:
          aH = 0;
          break;
        case 3:
          aM = 0;
          break;
        case 4:
          yy = 20;
          break;
        case 5:
          mmm = 1;
          break;
        case 6:
          dd = 1;
          break;
        case 7:
          h = 0;
          break;
        case 8:
          m = 0;
          break;
        case 9:
          chimeST = 0;
          break;
        case 10:
          bright = 0;
          display.setBrightness(bright);
          break;
      }
    }

  //press increment switch
    else if (modeST == LOW) { //change mode
      setMode++;
      if (setMode == 11) setMode = 0; 
    }
    else { //increment
      switch(setMode) {
        case 0:
          sDis[2] = digit[int(aH/10)]; //hhmm
          sDis[3] = digit[aH%10];
          sDis[4] = digit[int(aM/10)];
          sDis[5] = digit[aM%10];
          sDis[10] = digit[int(yy/10)];
          sDis[11] = digit[yy%10];
          dName = yearMonth[mmm-1];
          for (int i=0;i<3;i++) {
            sDis[i+13]=alphabet[dName[i]-65]; //mmm
          }
          sDis[17] = digit[int(dd/10)];
          sDis[18] = digit[dd%10];
          dName = weekDay[dow];
          for (int i=0;i<3;i++) {
            sDis[i+20]=alphabet[dName[i]-65]; //ddd
          }
          for (int i=0;i<21;i++) {
            display.setSegments(sDis+i, 4, 0); 
            delay(500);     
          }
          break;
        case 1: //set timer mode
          tmr++;
          if (tmr == 961) tmr = 0; //maximum 16 hour timer
          break;
        case 2: //set alarm hr mode
          aH++;
          if (aH == 25) aH = 0;
          break;
        case 3: //set alarm min mode
          aM++;
          if (aM == 60) aM = 0;
          break;
        case 4: //set year mode
          yy++;
          if (yy == 100) yy = 0;
          break;
        case 5: //set month mode
          mmm++;
          if (mmm == 13) mmm = 1;
          switch(mmm) {
            case 4:
            case 6:
            case 9:
            case 11:
              if (dd == 31) dd = 30;
              break;
            case 2:
              if ((yy%4) == 0) {
                if (dd >= 30) dd = 29;
              }
              else if (dd >= 29) dd = 28;
              break;
          }
          break;
        case 6: //set day mode
          dd++;
          switch(mmm) {
            case 1:
            case 3:
            case 5:
            case 7:
            case 8:
            case 10:
            case 12:
              if (dd == 32) dd = 1;
              break;
            case 4:
            case 6:
            case 9:
            case 11:
              if (dd >= 31) dd = 1;
              break;
            case 2:
              if ((yy%4) == 0) {
                if (dd >= 30) dd = 1;
              }
              else if (dd >= 29) dd = 1;
              break;
          }
          break;
        case 7: //set hr mode
          h++;
          if (h == 24) h = 0;
          break;
        case 8: //set min mode
          m++;
          if (m == 60) m = 0;
          timeStart = millis();
          cycleStart = millis();
          break;
        case 9: //set chime mode
          chimeST++;
          if (chimeST == 4) chimeST = 0;
          break;
        case 10: //set brightness
          bright++;
          if (bright == 8) bright = 0;
          display.setBrightness(bright);
          break;
      }
      dow=((yy)*365L+int((yy+3)/4)+monthDay[mmm-1]+(mmm>2 && yy%4==0)+dd-1+6)%7; //2000 Jan 01 was Saturday
    }

//press mode switch
    switch(setMode) {
      case 0:
        break;
      case 1: //set timer mode
        display.setSegments(alphabet+19, 1, 0); //t
        display.showNumberDec(tmr, true, 3, 1);
        break;
      case 2: //set alarm hr mode
        display.showNumberDec(aH, true, 2, 0);
        display.setSegments(symbol, 2, 2); // hh--
        break;
      case 3: //set alarm min mode
        display.setSegments(symbol, 2, 0); //--mm
        display.showNumberDec(aM, true, 2, 2);
        break;
      case 4: //set year mode
        display.setSegments(alphabet+24, 1, 0); //y
        display.showNumberDec(yy, true, 2, 2);
        break;
      case 5: //set month mode
        display.setSegments(alphabet+12, 1, 0); //M
        display.showNumberDec(mmm, true, 2, 2);
        break;
      case 6: //set day mode
        display.setSegments(alphabet+3, 1, 0); //d
        display.showNumberDec(dd, true, 2, 2);
        break;
      case 7: //set hr mode
        display.showNumberDecEx(h, 0b01000000, true, 2, 0);
        display.setSegments(symbol, 2, 2); //hh:--
        break;
      case 8: //set min mode
        display.setSegments(symbol, 1, 0); //--:mm
        display.setSegments(symbol+5, 1, 1);
        display.showNumberDec(m, true, 2, 2);
        break;
      case 9: //set chime mode
        display.setSegments(alphabet+2, 1, 0); //C
        display.showNumberDec(chimeST, false, 3, 1);
        break;
      case 10: //set brightness
        display.setSegments(alphabet+1, 1, 0); //B
        display.showNumberDec(bright, false, 3, 1);
        break;
    }
    cycle=0;
    delay(500);
  }
}
